    ADiff x1, x2, u1;
    output_file  = "ex2test.data";
    n_states     = 2;
    n_controls   = 1;
    n_initial    = 2;
    n_terminal   = 2;
    n_nodes      = 5;
    initial_time = 0.0;
    final_time   = 1.0;
    initial_constraint {
           x1 = y(1);
           x2 = y(2);
           u1 = u(1);
           Gamma(1) = x1-1.0;
           Gamma(2) = x2-1.0;
    }
    dynamic_equation_and_cost_function {
           x1 = y(1);
           x2 = y(2);
           u1 = u(1);
           F(1) = x2;
           F(2) = u1;
           L = 0.5*u1*u1;
    }
    terminal_condition {
           x1 = y(1);
           x2 = y(2);
           u1 = u(1);
           Psi(1) = x1;
           Psi(2) = x2;
    }
    solution_estimate {
           y0(1) = 1.0;
           y0(2) = 1.0;
    } 
